class Solution {
public:
    int arraySign(vector<int>& nums) {
   
    int p=0;
    int n=0;
    int z=0;

    for(int i=0;i<nums.size();i++){
    if(nums[i]>0)p++;
    else if(nums[i]<0)n++;
    else if(nums[i]==0)z++;
    }

    if(z==0){
     if(n%2==0)return 1;
     else{return -1;
     }

    }
  
    return 0;


    }
};
