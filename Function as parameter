// CALL BY VALUE 
#include<iostream>
using namespace std; 
void swap(int x,int y){
int temp=x;
x=y;
y=temp;
}
int main(){
    int a=10;
    int b=5;
  swap(a,b);
cout<<a<< " " << b;
}
// CALL BY ADDRESS 
#include<iostream>
using namespace std; 
void swap(int *x,int *y){
int temp=*x;
*x=*y;
*y=temp;
}
int main(){
    int a=10;
    int b=5;
  swap(&a,&b);
cout<<a<< " " << b;
}
// CALL BY REFERENCE 
#include<iostream>
using namespace std; 
void swap(int &x,int &y){
int temp=x;
x=y;
y=temp;
}
int main(){
    int a=10;
    int b=5;
  swap(a,b);
cout<<a<< " " << b;
} 
